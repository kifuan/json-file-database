{
    "name": "json-file-database",
    "version": "1.3.12",
    "description": "Lightweight Database on NodeJS by JSON files",
    "keywords": [
        "lightweight",
        "node",
        "database",
        "json"
    ],
    "main": "dist/src/index.js",
    "typings": "dist/src/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/kifuan/json-file-database"
    },
    "scripts": {
        "test": "tsc && ava",
        "prepublish": "typedoc --out docs src/index.ts && tsc"
    },
    "author": "kifuan",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^17.0.41",
        "ava": "^4.3.0",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.3"
    },
    "ava": {
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "files": [
            "test/*.test.ts"
        ]
    }
}
